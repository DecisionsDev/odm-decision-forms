{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "Response": {
      "type": "object",
      "properties": {
        "Fee": {
          "$ref": "#/definitions/BaggageFeeResult"
        }
      },
      "description": "Response for the execution of the decision service operation. Contains notably the output parameters that are returned by the execution.",
      "title": "Response"
    },
    "Customer": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string",
          "enum": [
            "Bronze",
            "Gold",
            "Silver",
            "Platinum",
            "None"
          ],
          "title": "Status"
        }
      },
      "title": "Customer"
    },
    "BaggageFeeResult": {
      "type": "object",
      "properties": {
        "totalBaggageFee": {
          "type": "number",
          "title": "Total Baggage Fee"
        },
        "status": {
          "type": "string",
          "enum": [
            "NoCharge",
            "AdditionalCharge",
            "Rejected"
          ],
          "title": "Status"
        },
        "discount": {
          "type": "string",
          "title": "Discount"
        },
        "reasons": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "title": "Reasons"
        }
      },
      "title": "Baggage Fee Result"
    },
    "Request": {
      "type": "object",
      "properties": {
        "Booking": {
          "$ref": "#/definitions/Booking"
        },
        "Customer": {
          "$ref": "#/definitions/Customer"
        },
        "Fee_per_extra_kilogram": {
          "type": "number",
          "title": "Fee Per Extra Kilogram"
        },
        "Maximum_spec_for_a_piece": {
          "$ref": "#/definitions/BaggageItem"
        }
      },
      "description": "Request for the execution of the decision service operation. Contains notably the input parameters that are used for the execution.",
      "title": "Request"
    },
    "Error": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "description": "HTTP error code.",
          "title": "Code"
        },
        "message": {
          "type": "string",
          "description": "Error message.",
          "title": "Message"
        },
        "details": {
          "type": "string",
          "description": "Detailed error message.",
          "title": "Details"
        },
        "errorCode": {
          "type": "string",
          "description": "Product error code.",
          "title": "Error Code"
        }
      },
      "description": "Error occurring when invoking the execution of the decision service operation.",
      "title": "Error"
    },
    "Booking": {
      "type": "object",
      "properties": {
        "baggageItems": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/BaggageItem"
          },
          "title": "Baggage Items"
        },
        "fareClass": {
          "type": "string",
          "enum": [
            "Economy",
            "Business",
            "PremiumEconomy",
            "First"
          ],
          "title": "Fare Class"
        }
      },
      "title": "Booking"
    },
    "BaggageItem": {
      "type": "object",
      "properties": {
        "depth": {
          "type": "number",
          "title": "Depth"
        },
        "width": {
          "type": "number",
          "title": "Width"
        },
        "height": {
          "type": "number",
          "title": "Height"
        },
        "weight": {
          "type": "number",
          "title": "Weight"
        }
      },
      "title": "Baggage Item"
    }
  },
  "type": "object",
  "properties": {
    "Booking": {
      "$ref": "#/definitions/Booking"
    },
    "Customer": {
      "$ref": "#/definitions/Customer"
    },
    "Fee_per_extra_kilogram": {
      "type": "number",
      "title": "Fee Per Extra Kilogram"
    },
    "Maximum_spec_for_a_piece": {
      "$ref": "#/definitions/BaggageItem"
    }
  }
}