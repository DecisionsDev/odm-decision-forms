{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "Borrower": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string",
          "title": "First Name"
        },
        "lastName": {
          "type": "string",
          "title": "Last Name"
        },
        "birthDate": {
          "type": "string",
          "format": "date-time",
          "title": "Birth Date"
        },
        "yearlyIncome": {
          "type": "integer",
          "title": "Yearly Income"
        },
        "zipCode": {
          "type": "string",
          "title": "Zip Code"
        },
        "creditScore": {
          "type": "integer",
          "title": "Credit Score"
        },
        "spouse": {
          "type": "string",
          "title": "Spouse",
          "description": "Warning: replaced cyclic reference (Borrower => spouse: Borrower) with a string property"
        },
        "latestBankruptcy": {
          "$ref": "#/definitions/Bankruptcy"
        },
        "SSN": {
          "$ref": "#/definitions/SSN"
        }
      },
      "title": "Borrower"
    },
    "Bankruptcy": {
      "type": "object",
      "properties": {
        "date": {
          "type": "string",
          "format": "date-time",
          "title": "Date"
        },
        "chapter": {
          "type": "integer",
          "title": "Chapter"
        },
        "reason": {
          "type": "string",
          "title": "Reason"
        }
      },
      "title": "Bankruptcy"
    },
    "SSN": {
      "type": "object",
      "properties": {
        "areaNumber": {
          "type": "string",
          "title": "Area Number"
        },
        "groupCode": {
          "type": "string",
          "title": "Group Code"
        },
        "serialNumber": {
          "type": "string",
          "title": "Serial Number"
        }
      },
      "title": "Ssn"
    },
    "Loan": {
      "type": "object",
      "properties": {
        "numberOfMonthlyPayments": {
          "type": "integer",
          "title": "Number Of Monthly Payments"
        },
        "startDate": {
          "type": "string",
          "format": "date-time",
          "title": "Start Date"
        },
        "amount": {
          "type": "integer",
          "title": "Amount"
        },
        "loanToValue": {
          "type": "number",
          "title": "Loan To Value"
        }
      },
      "title": "Loan"
    }
  },
  "type": "object",
  "properties": {
    "Borrower": {
      "$ref": "#/definitions/Borrower"
    },
    "Loan": {
      "$ref": "#/definitions/Loan"
    },
    "current_time": {
      "type": "string",
      "format": "date-time",
      "title": "Current Time"
    }
  }
}