{
  "name": "odm-decision-forms",
  "version": "0.1.24",
  "author": "Antoine Melki",
  "license": "Apache-2.0",
  "description": "A React-based UI to init and run Hosted Transparent Decision Services",
  "repository": {
    "type": "git",
    "url": "https://github.com/ODMDev/odm-decision-forms"
  },
  "bugs": {
    "email": "Antoine.Melki@ibm.com"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "bin": {
    "odm-decision-forms": "./src/server/index.js"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "dist",
    "lib",
    "lib-esm"
  ],
  "keywords": [
    "odm",
    "htds",
    "decision"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "dev": "node src/server/index --env development",
    "start": "node src/server/index --env production",
    "clean": "shx rm -rf _bundles lib lib-esm dist",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build:lib": "rimraf lib && npm run compile && cross-env NODE_ENV=production webpack --config ./webpack.lib.config.js --progress --profile --colors",
    "build": "npm run clean && npm run build:lib && npm run build:dist",
    "patch": "npm build && npm version patch && npm publish",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "compile:commonjs": "tsc -m commonjs --outDir lib",
    "compile:es6": "tsc -m es6 --outDir lib-esm",
    "compile": "npm run compile:commonjs && npm run compile:es6"
  },
  "dependencies": {
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-node6": "^11.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.1",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "cors": "^2.8.4",
    "decamelize": "^1.2.0",
    "decamelize-keys": "^1.1.0",
    "es6-map": "^0.1.5",
    "es6-object-assign": "^1.1.0",
    "es6-promise-promise": "^1.0.0",
    "event-source-polyfill": "0.0.11",
    "eventsource": "^1.0.5",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "flux": "^3.1.3",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.4",
    "object-path": "^0.11.4",
    "object.values": "^1.0.4",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^16.1.1",
    "react-json-pretty": "^1.6.3",
    "react-jsonschema-form": "^0.50.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-transform-hmr": "^1.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "redux-watch": "^1.1.1",
    "solar-popup": "^0.4.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.82",
    "@types/es6-promise": "0.0.33",
    "@types/jest": "^20.0.8",
    "@types/jquery": "^3.2.13",
    "@types/react-dnd": "^2.0.34",
    "@types/react-redux": "^5.0.8",
    "shx": "^0.2.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-jscs": "^3.0.0-beta1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jscs": "^3.0.7",
    "jsdoc-to-markdown": "^3.0.0",
    "jsdom": "^11.1.0",
    "json-loader": "^0.5.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "marked": "^0.3.6",
    "mocha": "^3.5.3",
    "mocha-jsdom": "^1.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "to-markdown": "^3.0.3",
    "ts-jest": "^21.0.1",
    "typescript": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.13.1"
  }
}
